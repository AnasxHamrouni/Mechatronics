<?xml version="1.0"?>
<robot name="double_pendulum">

  <mujoco>
    <compiler balanceinertia="true" discardvisual="false" meshdir="meshes" fusestatic="false"/>
  </mujoco>

  <link name="world"/>

    <link name="link_1">
    <visual>
      <geometry>
        <cylinder radius="0.02" length="0.03"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="green">
        <color rgba="0.3 0.9 0.3 0.4"/>
      </material>
    </visual>
    <visual>
      <geometry>
        <box size="0.02 0.02 0.10"/>
      </geometry>
      <origin xyz="0 0 -0.05" rpy="0 0 0"/>
      <material name="gray">
        <color rgba="0.6 0.6 0.6 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.02 0.02 0.10"/>
      </geometry>
      <origin xyz="0 0 -0.05" rpy="0 0 0"/>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.001" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <joint name="joint_1" type="revolute">
    <parent link="world"/>
    <child link="link_1"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="5"/>
  </joint>

  <link name="link_2">
    <visual>
      <geometry>
        <cylinder radius="0.02" length="0.03"/>
      </geometry>
      <origin xyz="0 0 0" rpy="1.5708 0 0"/>
      <material name="green">
        <color rgba="0.3 0.9 0.3 0.4"/>
      </material>
    </visual>
    <visual>
      <geometry>
        <box size="0.02 0.02 0.10"/>
      </geometry>
      <origin xyz="0.05 0 0" rpy="0 1.5708 0"/>
      <material name="gray">
        <color rgba="0.6 0.6 0.6 1.0"/>
      </material>
    </visual>
    <collision>
      <geometry>
        <box size="0.02 0.02 0.10"/>
      </geometry>
      <origin xyz="0.05 0 0" rpy="0 1.5708 0"/>
    </collision>
    <inertial>
      <mass value="1.0"/>
      <inertia ixx="0.001" iyy="0.001" izz="0.001" ixy="0" ixz="0" iyz="0"/>
    </inertial>
  </link>

  <joint name="joint_2" type="revolute">
    <parent link="link_1"/>
    <child link="link_2"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit lower="-3.14" upper="3.14" effort="10" velocity="5"/>
  </joint>

</robot>